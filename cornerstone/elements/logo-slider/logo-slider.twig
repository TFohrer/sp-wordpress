{{ dump() }}
<div class="logo-showcase-slider logo-showcase-slider--{{
  random()
  }} {{ classes|join }}">
  <div class="logo-showcase-slider__container">
    {% for item in items %}
      <div class="logo-showcase-slider__slide">
        <picture>
          <source srcset="{{ item.image.src|towebp|resize(200) }}"
            type="image/webp" />
          <source srcset="{{ item.image.src|tojpg|resize(200) }}"
            type="image/jpeg" />
          <img src="{{ item.image.src|tojpg }}" alt="{{ item.image.alt }}" />
        </picture>
        {# <img src="{{ item.image.src|tojpg }}" alt="{{ item.image.alt }}" /> #}
      </div>
    {% endfor %}
  </div>
</div>
